@model IEnumerable<RegistroNotas.PeriodoLectivo>

@{
    Layout = null;
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NombrePeriodo)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombrePeriodo)
            </td>
        </tr>
    }

</table>
